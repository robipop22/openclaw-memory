{
"name": "@poprobertdaniel/openclaw-memory",
  "version": "0.1.2",
  "description": "Triple-layer memory system for AI agents â€” SQLite + Qdrant + Postgres/AGE",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.js"
      },
      "require": {
        "types": "./dist/server.d.cts",
        "default": "./dist/server.cjs"
      }
    }
  },
  "bin": {
    "openclaw-memory": "./dist/cli/index.js"
  },
  "files": [
    "dist/",
    "templates/",
    "docker/",
    "skill/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "bun --watch run src/server.ts",
    "start": "bun run src/server.ts",
    "test": "echo 'No tests yet'",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "memory",
    "agents",
    "sqlite",
    "qdrant",
    "graph",
    "knowledge-graph",
    "semantic-search",
    "vector-database",
    "openclaw",
    "llm",
    "rag",
    "apache-age",
    "triple-layer"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robipop22/openclaw-memory"
  },
  "homepage": "https://github.com/robipop22/openclaw-memory#readme",
  "bugs": {
    "url": "https://github.com/robipop22/openclaw-memory/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.2.0",
    "commander": "^13.0.0",
    "elysia": "^1.2.0",
    "openai": "^4.80.0",
    "picocolors": "^1.1.0",
    "uuid": "^11.1.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^11.0.0"
  },
  "peerDependencies": {
    "@qdrant/js-client-rest": ">=1.0.0",
    "pg": "^8.13.0"
  },
  "peerDependenciesMeta": {
    "@qdrant/js-client-rest": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  }
}
